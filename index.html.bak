    <style>
        .sr-only {
            position: absolute;
            width: 1px;
            height: 1px;
            padding: 0;
            margin: -1px;
            overflow: hidden;
            clip: rect(0, 0, 0, 0);
            white-space: nowrap;
            border: 0;
        }
        .sr-only:focus-within,
        .sr-only:focus-visible {
            position: static;
            width: auto;
            height: auto;
            margin: 0;
            padding: 0.5rem 1rem;
            clip: auto;
            background: rgba(15, 23, 42, 0.8);
            color: #f8fafc;
            border-radius: 8px;
            box-shadow: 0 12px 50px rgba(15, 23, 42, 0.4);
        }
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: 'Inter', sans-serif;
            overflow-x: hidden;
            background: linear-gradient(135deg, #0f172a 0%, #1e293b 100%);
            color: #f8fafc;
        }
        
        .hero-gradient {
            background: linear-gradient(135deg, #3b82f6 0%, #8b5cf6 50%, #ec4899 100%);
            background-size: 400% 400%;
            animation: gradientShift 8s ease infinite;
        }
        
        @keyframes gradientShift {
            0% { background-position: 0% 50%; }
            50% { background-position: 100% 50%; }
            100% { background-position: 0% 50%; }
        }
        
        .floating-card {
            animation: float 6s ease-in-out infinite;
        }
        
        @keyframes float {
            0%, 100% { transform: translateY(0px); }
            50% { transform: translateY(-20px); }
        }
        
        .grid-pattern {
            background-image: 
                linear-gradient(rgba(59, 130, 246, 0.1) 1px, transparent 1px),
                linear-gradient(90deg, rgba(59, 130, 246, 0.1) 1px, transparent 1px);
            background-size: 50px 50px;
        }
        
        .tech-card {
            background: rgba(255, 255, 255, 0.02);
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.1);
            transition: all 0.3s ease;
        }
        
        .tech-card:hover {
            transform: translateY(-8px);
            border-color: #3b82f6;
            box-shadow: 0 20px 40px rgba(59, 130, 246, 0.2);
        }
        
        .pulse-dot {
            animation: pulse 2s infinite;
        }
        
        @keyframes pulse {
            0% { opacity: 0.4; transform: scale(1); }
            50% { opacity: 1; transform: scale(1.1); }
            100% { opacity: 0.4; transform: scale(1); }
        }
        
        .automation-flow {
            position: relative;
            overflow: hidden;
        }
        
        .flow-line {
            position: absolute;
            top: 50%;
            left: 0;
            right: 0;
            height: 2px;
            background: linear-gradient(90deg, transparent, #3b82f6, transparent);
        }
        
        .flow-node {
            animation: flowMove 3s linear infinite;
        }
        
        @keyframes flowMove {
            0% { transform: translateX(-100%); }
            100% { transform: translateX(400%); }
        }
        
        .glass-effect {
            background: rgba(255, 255, 255, 0.05);
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.1);
        }

        /* Feature Canvas Mock - matches actual frontend */
        .canvas-mock {
            position: relative;
            width: 100%;
            height: 600px;
            background: linear-gradient(135deg, #0b1628 0%, #0f172a 50%, #1e293b 100%);
            border-radius: 24px;
            overflow: hidden;
            box-shadow: inset 0 0 48px rgba(15, 18, 26, 0.6);
        }

        .canvas-mock::-webkit-scrollbar {
            display: none;
        }

        .canvas-mock {
            -ms-overflow-style: none;
            scrollbar-width: none;
        }

        .canvas-mock__grid {
            position: absolute;
            inset: 0;
            background-image: 
                linear-gradient(rgba(59, 130, 246, 0.08) 1px, transparent 1px),
                linear-gradient(90deg, rgba(59, 130, 246, 0.08) 1px, transparent 1px);
            background-size: 50px 50px;
            opacity: 0.4;
        }

        .canvas-mock__nodes {
            position: absolute;
            inset: 0;
            padding: 80px 40px;
            display: block;
            z-index: 2;
        }

        .canvas-mock__node {
            position: absolute;
            left: var(--x, 50%);
            top: var(--y, 50%);
            min-width: 130px;
            max-width: 150px;
            min-height: 80px;
            display: flex;
            flex-direction: column;
            gap: 5px;
            border-radius: 12px;
            border: 1px solid rgba(148, 163, 184, 0.25);
            background: radial-gradient(circle at top, rgba(46, 56, 77, 0.95), rgba(17, 24, 39, 0.94));
            box-shadow: 0 24px 50px rgba(15, 23, 42, 0.45);
            padding: 10px 12px;
            color: #e2e8f0;
            transform: translate(-50%, -50%);
            cursor: grab;
            transition: transform 0.18s ease, box-shadow 0.3s ease, border-color 0.3s ease;
        }

        .canvas-mock__node.is-dragging {
            cursor: grabbing;
            box-shadow: 0 30px 60px rgba(59, 130, 246, 0.35);
        }

        .canvas-mock__node:hover {
            transform: translate(-50%, -50%) scale(1.02);
            box-shadow: 0 28px 60px rgba(59, 130, 246, 0.3);
            border-color: rgba(94, 234, 212, 0.5);
        }

        body.is-dragging,
        body.is-dragging * {
            user-select: none !important;
        }

        .canvas-mock__node::after {
            content: '';
            position: absolute;
            inset: 0;
            border-radius: 18px;
            border: 1px solid rgba(148, 163, 184, 0.12);
            pointer-events: none;
        }

        .canvas-mock__node-header {
            display: flex;
            align-items: flex-start;
            justify-content: space-between;
            gap: 12px;
        }

        .canvas-mock__node-title {
            font-weight: 600;
            font-size: 0.85rem;
            color: #f8fafc;
            line-height: 1.2;
            flex: 1;
        }

        .canvas-mock__drag-handle {
            display: inline-flex;
            align-items: center;
            justify-content: center;
            width: 20px;
            height: 20px;
            border-radius: 999px;
            background: rgba(30, 41, 59, 0.9);
            color: rgba(226, 232, 240, 0.85);
            font-size: 0.7rem;
            font-weight: 600;
            cursor: grab;
            box-shadow: inset 0 0 0 1px rgba(148, 163, 184, 0.35);
            order: -1;
            flex-shrink: 0;
        }

        .canvas-mock__node-desc {
            font-size: 0.7rem;
            color: rgba(203, 213, 225, 0.92);
            line-height: 1.3;
        }

        .canvas-mock__handle {
            position: absolute;
            width: 14px;
            height: 14px;
            border-radius: 999px;
            border: 2px solid #0f172a;
            z-index: 10;
            transition: transform 0.18s ease;
            box-shadow: 0 0 6px rgba(0, 0, 0, 0.5);
        }

        .canvas-mock__handle--left {
            left: 0;
            top: 50%;
            transform: translate(-50%, -50%);
            background: rgba(148, 163, 184, 0.6);
            border-color: rgba(100, 116, 139, 1);
        }

        .canvas-mock__handle--right {
            right: 0;
            top: 50%;
            transform: translate(50%, -50%);
            background: rgba(45, 212, 191, 0.5);
            border-color: rgba(20, 184, 166, 1);
        }

        .canvas-mock__handle:hover {
            transform: translate(-50%, -50%) scale(1.15);
        }

        .canvas-mock__handle--right:hover {
            transform: translate(50%, -50%) scale(1.15);
        }

        .canvas-mock__handle--top {
            right: 0;
            top: 25%;
            transform: translate(50%, -50%);
            background: rgba(45, 212, 191, 0.5);
            border-color: rgba(20, 184, 166, 1);
        }

        .canvas-mock__handle--middle {
            right: 0;
            top: 50%;
            transform: translate(50%, -50%);
            background: rgba(45, 212, 191, 0.5);
            border-color: rgba(20, 184, 166, 1);
        }

        .canvas-mock__handle--bottom {
            right: 0;
            top: 75%;
            transform: translate(50%, -50%);
            background: rgba(45, 212, 191, 0.5);
            border-color: rgba(20, 184, 166, 1);
        }

        .canvas-mock__handle--top:hover,
        .canvas-mock__handle--middle:hover,
        .canvas-mock__handle--bottom:hover {
            transform: translate(50%, -50%) scale(1.15);
        }

        .canvas-mock__handle-label {
            position: absolute;
            right: -42px;
            top: 50%;
            transform: translateY(-50%);
            font-size: 0.6rem;
            color: rgba(203, 213, 225, 0.95);
            white-space: nowrap;
            font-weight: 600;
            text-shadow: 0 1px 2px rgba(0, 0, 0, 0.5);
        }

        .canvas-mock__edge {
            position: absolute;
            pointer-events: none;
            z-index: 1;
        }

        .canvas-mock__edge path {
            animation: dashFlow 2s linear infinite;
        }

        @keyframes dashFlow {
            0% {
                stroke-dashoffset: 28;
            }
            100% {
                stroke-dashoffset: 0;
            }
        }

        .canvas-mock__edge-gradient {
            stop:nth-child(1) { stop-color: #38bdf8; stop-opacity: 0.85; }
            stop:nth-child(2) { stop-color: #6366f1; stop-opacity: 0.95; }
            stop:nth-child(3) { stop-color: #a855f7; stop-opacity: 0.9; }
        }

        @media (max-width: 1280px) {
            .canvas-mock {
                height: 550px;
            }

            .canvas-mock__nodes {
                padding: 70px 30px;
            }

            .canvas-mock__node {
                min-width: 120px;
                max-width: 140px;
                min-height: 75px;
            }
        }
        
        @media (max-width: 1024px) {
            .canvas-mock {
                height: 620px;
            }

            .canvas-mock__grid {
                background-size: 40px 40px;
            }

            .canvas-mock__nodes {
                padding: 48px 24px;
                gap: 28px;
            }

            .canvas-mock__node {
                min-width: clamp(120px, 42vw, 180px);
                max-width: clamp(140px, 46vw, 220px);
                min-height: 90px;
                font-size: 0.85rem;
            }

            .canvas-mock__node-title {
                font-size: 0.82rem;
            }

            .canvas-mock__node-desc {
                font-size: 0.65rem;
            }
        }
        
        @media (max-width: 640px) {
            .canvas-mock {
                height: 640px;
                min-width: 520px;
                overflow: auto;
                -webkit-overflow-scrolling: touch;
            }

            .canvas-mock__grid {
                background-size: 32px 32px;
            }

            .canvas-mock__nodes {
                padding: 36px 18px;
                gap: 22px;
            }

            .canvas-mock__node {
                min-width: clamp(130px, 60vw, 210px);
                max-width: clamp(150px, 68vw, 240px);
                min-height: 95px;
            }

            .canvas-mock__node-title {
                font-size: 0.78rem;
            }
            
            .canvas-mock__node-desc {
                font-size: 0.68rem;
            }
        }
    </style>
