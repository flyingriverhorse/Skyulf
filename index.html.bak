    <!-- Scroll Buttons -->
    <button id="scroll-to-top" class="fixed right-6 bottom-24 bg-gradient-to-r from-blue-500 to-purple-600 text-white p-3 rounded-full shadow-lg opacity-0 pointer-events-none transition-all duration-300 hover:scale-110 z-50 group" onclick="scrollToTop()">
        <svg class="w-6 h-6 group-hover:animate-bounce" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 10l7-7m0 0l7 7m-7-7v18"></path>
        </svg>
    </button>
    
    <button id="scroll-to-bottom" class="fixed right-6 bottom-6 bg-gradient-to-r from-purple-600 to-pink-600 text-white p-3 rounded-full shadow-lg opacity-0 pointer-events-none transition-all duration-300 hover:scale-110 z-50 group" onclick="scrollToBottom()">
        <svg class="w-6 h-6 group-hover:animate-bounce" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3"></path>
        </svg>
    </button>

    <script>
        AOS.init({
            duration: 800,
            once: true,
            offset: 100
        });
        
        feather.replace();
        
        // Animation for automation flow
        document.addEventListener('DOMContentLoaded', function() {
            const productTourVideo = document.getElementById('product-tour-video');
            const speedButtons = document.querySelectorAll('[data-video-speed]');

            const productTourSkeleton = document.getElementById('product-tour-video-skeleton');

            if (productTourVideo) {
                productTourVideo.controls = true;
                productTourVideo.load();

                productTourVideo.addEventListener('error', () => {
                    console.warn('Skyulf product tour video could not be loaded. Check that /static/video/skyulf-product-tour.mp4 exists.');
                });

                const revealVideo = () => {
                    productTourVideo.classList.add('opacity-100');
                    if (productTourSkeleton) {
                        productTourSkeleton.classList.remove('animate-pulse');
                        productTourSkeleton.classList.add('opacity-0');
                        setTimeout(() => {
                            productTourSkeleton.classList.add('hidden');
                        }, 500);
                    }
                };

                productTourVideo.addEventListener('loadeddata', revealVideo, { once: true });
                productTourVideo.addEventListener('play', revealVideo, { once: true });
                productTourVideo.addEventListener('canplay', revealVideo, { once: true });
            }

            if (productTourVideo && speedButtons.length) {
                const activeClasses = ['bg-white/20', 'text-white'];
                const inactiveClasses = ['text-gray-300'];

                const setActiveButton = (button) => {
                    speedButtons.forEach(btn => {
                        activeClasses.forEach(cls => btn.classList.remove(cls));
                        inactiveClasses.forEach(cls => btn.classList.add(cls));
                    });

                    activeClasses.forEach(cls => button.classList.add(cls));
                    inactiveClasses.forEach(cls => button.classList.remove(cls));
                };

                speedButtons.forEach(button => {
                    button.addEventListener('click', () => {
                        const rate = Number.parseFloat(button.dataset.videoSpeed);
                        if (!Number.isNaN(rate)) {
                            productTourVideo.playbackRate = rate;
                        }
                        setActiveButton(button);
                    });
                });

                const defaultRate = Number.parseFloat(speedButtons[0].dataset.videoSpeed);
                if (!Number.isNaN(defaultRate)) {
                    productTourVideo.playbackRate = defaultRate;
                }
                setActiveButton(speedButtons[0]);
            }

            const flowNode = document.querySelector('.flow-node');
            if (flowNode) {
                anime({
                    targets: flowNode,
                    left: ['0%', '100%'],
                    duration: 3000,
                    loop: true,
                    easing: 'linear'
                });
            }
            
            // Add floating animation to multiple elements
            const floatingElements = document.querySelectorAll('.floating-card');
            floatingElements.forEach((el, index) => {
                anime({
                    targets: el,
                    translateY: [-10, 10],
                    duration: 2000 + index * 500,
                    loop: true,
                    direction: 'alternate',
                    easing: 'easeInOutSine'
                });
            });

            const canvas = document.querySelector('.canvas-mock');
            if (canvas) {
                const svg = canvas.querySelector('.canvas-mock__edge');
                const nodeElements = Array.from(canvas.querySelectorAll('.canvas-mock__node'));

                if (svg && nodeElements.length) {
                    const connectionConfigs = [
                        { id: 'source->split', from: 'source', fromHandle: '.canvas-mock__handle--right', to: 'split', toHandle: '.canvas-mock__handle--left' },
                        { id: 'split-top->trainer', from: 'split', fromHandle: '.canvas-mock__handle--top', to: 'trainer', toHandle: '.canvas-mock__handle--left' },
                        { id: 'split-middle->trainer', from: 'split', fromHandle: '.canvas-mock__handle--middle', to: 'trainer', toHandle: '.canvas-mock__handle--left' },
                        { id: 'split-bottom->trainer', from: 'split', fromHandle: '.canvas-mock__handle--bottom', to: 'trainer', toHandle: '.canvas-mock__handle--left' }
                    ];

                    const nodeMap = new Map(nodeElements.map(node => [node.dataset.nodeId, node]));
                    const connections = connectionConfigs
                        .map(cfg => ({
                            path: svg.querySelector(`[data-connection="${cfg.id}"]`),
                            sourceHandle: nodeMap.get(cfg.from)?.querySelector(cfg.fromHandle),
                            targetHandle: nodeMap.get(cfg.to)?.querySelector(cfg.toHandle)
                        }))
                        .filter(conn => conn.path && conn.sourceHandle && conn.targetHandle);

                    const updateConnections = () => {
                        svg.style.opacity = window.innerWidth <= 640 ? '0.85' : '1';
                        const canvasRect = canvas.getBoundingClientRect();
                        svg.setAttribute('viewBox', `0 0 ${canvasRect.width} ${canvasRect.height}`);
                        svg.setAttribute('width', canvasRect.width);
                        svg.setAttribute('height', canvasRect.height);
                        svg.setAttribute('preserveAspectRatio', 'none');

                        connections.forEach(({ path, sourceHandle, targetHandle }) => {
                            const sourceRect = sourceHandle.getBoundingClientRect();
                            const targetRect = targetHandle.getBoundingClientRect();

                            const startX = (sourceRect.left + sourceRect.width / 2) - canvasRect.left;
                            const startY = (sourceRect.top + sourceRect.height / 2) - canvasRect.top;
                            const endX = (targetRect.left + targetRect.width / 2) - canvasRect.left;
                            const endY = (targetRect.top + targetRect.height / 2) - canvasRect.top;

                            const deltaX = Math.max(Math.abs(endX - startX) * 0.35, 60);

                            const pathData = `M ${startX} ${startY} C ${startX + deltaX} ${startY}, ${endX - deltaX} ${endY}, ${endX} ${endY}`;
                            path.setAttribute('d', pathData);
                        });
                    };

                    const applyInitialLayout = ({ force = false } = {}) => {
                        if (force) {
                            nodeElements.forEach(node => delete node.dataset.hasCustomPosition);
                        }

                        const canvasRect = canvas.getBoundingClientRect();
                        const useCompactLayout = canvasRect.width < 900;

                        nodeElements.forEach((node, index) => {
                            if (!force && node.dataset.hasCustomPosition === 'true') {
                                return;
                            }

                            let xPercent;
                            let yPercent;

                            if (useCompactLayout) {
                                const positions = [
                                    { x: 30, y: 25 },
                                    { x: 55, y: 50 },
                                    { x: 75, y: 75 }
                                ];
                                xPercent = positions[index].x;
                                yPercent = positions[index].y;
                            } else {
                                const positions = [
                                    { x: 18, y: 25 },
                                    { x: 48, y: 50 },
                                    { x: 80, y: 75 }
                                ];
                                xPercent = positions[index].x;
                                yPercent = positions[index].y;
                            }

                            node.style.setProperty('--x', `${xPercent}%`);
                            node.style.setProperty('--y', `${yPercent}%`);
                        });

                        requestAnimationFrame(() => {
                            requestAnimationFrame(() => {
                                updateConnections();
                            });
                        });
                    };

                    applyInitialLayout({ force: true });
                    window.addEventListener('resize', () => {
                        requestAnimationFrame(() => applyInitialLayout());
                    });

                    if (window.matchMedia('(pointer:fine)').matches) {
                        nodeElements.forEach(node => {
                            node.addEventListener('pointerdown', event => {
                                if (event.button !== 0) {
                                    return;
                                }

                                event.preventDefault();

                                const pointerId = event.pointerId;
                                node.setPointerCapture(pointerId);
                                node.classList.add('is-dragging');
                                document.body.classList.add('is-dragging');

                                const nodeRect = node.getBoundingClientRect();
                                const width = nodeRect.width;
                                const height = nodeRect.height;
                                const offsetX = event.clientX - (nodeRect.left + width / 2);
                                const offsetY = event.clientY - (nodeRect.top + height / 2);
                                let didDrag = false;

                                const handleMove = moveEvent => {
                                    const rect = canvas.getBoundingClientRect();
                                    let centerX = moveEvent.clientX - offsetX;
                                    let centerY = moveEvent.clientY - offsetY;

                                    const minX = rect.left + width / 2;
                                    const maxX = rect.right - width / 2;
                                    const minY = rect.top + height / 2;
                                    const maxY = rect.bottom - height / 2;

                                    centerX = Math.min(Math.max(centerX, minX), maxX);
                                    centerY = Math.min(Math.max(centerY, minY), maxY);

                                    const percentX = ((centerX - rect.left) / rect.width) * 100;
                                    const percentY = ((centerY - rect.top) / rect.height) * 100;

                                    node.style.setProperty('--x', `${percentX.toFixed(2)}%`);
                                    node.style.setProperty('--y', `${percentY.toFixed(2)}%`);

                                    requestAnimationFrame(() => updateConnections());
                                    didDrag = true;
                                };

                                const handleUp = () => {
                                    node.releasePointerCapture(pointerId);
                                    node.removeEventListener('pointermove', handleMove);
                                    node.removeEventListener('pointerup', handleUp);
                                    node.removeEventListener('pointercancel', handleUp);
                                    node.classList.remove('is-dragging');
                                    document.body.classList.remove('is-dragging');
                                    if (didDrag) {
                                        node.dataset.hasCustomPosition = 'true';
                                    }
                                    updateConnections();
                                };

                                node.addEventListener('pointermove', handleMove);
                                node.addEventListener('pointerup', handleUp);
                                node.addEventListener('pointercancel', handleUp);
                            });
                        });
                    }
                }
            }
        });

        // FAQ Toggle Function
        function toggleFAQ(button) {
            const faqItem = button.closest('.faq-item');
            const answer = faqItem.querySelector('.faq-answer');
            const icon = faqItem.querySelector('.faq-icon');
            const isOpen = answer.style.maxHeight && answer.style.maxHeight !== '0px';
            
            if (isOpen) {
                answer.style.maxHeight = '0px';
                icon.style.transform = 'rotate(0deg)';
            } else {
                answer.style.maxHeight = answer.scrollHeight + 'px';
                icon.style.transform = 'rotate(180deg)';
            }
        }

        // FAQ Search Function
        document.addEventListener('DOMContentLoaded', function() {
            const searchInput = document.getElementById('faq-search');
            const faqItems = document.querySelectorAll('.faq-item');
            const noResults = document.getElementById('faq-no-results');

            if (searchInput) {
                searchInput.addEventListener('input', function(e) {
                    const searchTerm = e.target.value.toLowerCase().trim();
                    let visibleCount = 0;

                    faqItems.forEach(item => {
                        const question = item.querySelector('.faq-question h3').textContent.toLowerCase();
                        const answer = item.querySelector('.faq-answer').textContent.toLowerCase();
                        
                        if (question.includes(searchTerm) || answer.includes(searchTerm)) {
                            item.style.display = 'block';
                            visibleCount++;
                        } else {
                            item.style.display = 'none';
                            // Close the item if it was open
                            const answerDiv = item.querySelector('.faq-answer');
                            const icon = item.querySelector('.faq-icon');
                            if (answerDiv) {
                                answerDiv.style.maxHeight = '0px';
                                icon.style.transform = 'rotate(0deg)';
                            }
                        }
                    });

                    // Show/hide no results message
                    if (visibleCount === 0 && searchTerm !== '') {
                        noResults.classList.remove('hidden');
                    } else {
                        noResults.classList.add('hidden');
                    }
                });
            }
        });

        // Scroll Button Functions
        function scrollToTop() {
            window.scrollTo({
                top: 0,
                behavior: 'smooth'
            });
        }

        function scrollToBottom() {
            window.scrollTo({
                top: document.body.scrollHeight,
                behavior: 'smooth'
            });
        }

        // Show/hide scroll buttons based on scroll position
        window.addEventListener('scroll', function() {
            const scrollToTopBtn = document.getElementById('scroll-to-top');
            const scrollToBottomBtn = document.getElementById('scroll-to-bottom');
            const scrollPosition = window.scrollY;
            const windowHeight = window.innerHeight;
            const documentHeight = document.body.scrollHeight;
            
            // Show top button after scrolling down 300px
            if (scrollPosition > 300) {
                scrollToTopBtn.style.opacity = '1';
                scrollToTopBtn.style.pointerEvents = 'auto';
            } else {
                scrollToTopBtn.style.opacity = '0';
                scrollToTopBtn.style.pointerEvents = 'none';
            }
            
            // Show bottom button when not at the bottom (within 100px of bottom)
            if (scrollPosition + windowHeight < documentHeight - 100) {
                scrollToBottomBtn.style.opacity = '1';
                scrollToBottomBtn.style.pointerEvents = 'auto';
            } else {
                scrollToBottomBtn.style.opacity = '0';
                scrollToBottomBtn.style.pointerEvents = 'none';
            }
        });
    </script>
